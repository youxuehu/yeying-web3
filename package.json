{
  "name": "@yeying-community/yeying-web3",
  "version": "0.0.38",
  "author": "yeying.community@gmail.com",
  "description": "A web3 crypto library",
  "main": "dist/yeying-web3.cjs.js",
  "module": "dist/yeying-web3.esm.js",
  "browser": "dist/yeying-web3.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rm -rf dist && rollup -c",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "typedoc --out docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeying-community/yeying-web3.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yeying-community/yeying-web3/issues"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/tests/**/?(*.)(ut|it).ts?(x)"
    ]
  },
  "homepage": "https://github.com/yeying-community/yeying-web3#readme",
  "dependencies": {
    "elliptic": "^6.6.1",
    "ethers": "^6.13.4",
    "google-protobuf": "^3.21.4",
    "luxon": "^3.7.1",
    "ts-proto": "^2.7.0",
    "@waku/sdk": "^0.0.35",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/elliptic": "^6.4.18",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.6.2",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rollup": "^4.28.1",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.2",
    "@types/jsonwebtoken": "^9.0.3"
  }
}
